    font-family: inherit;
  }

  .abhinaya-track-order__search-btn:active {
    opacity: 0.85;
  }

  .abhinaya-track-order__search-btn:disabled {
    opacity: 0.5;
    cursor: not-allowed;
  }

  /* ── Error State ── */
  .abhinaya-track-order__error {
    display: flex;
    align-items: center;
    gap: 12px;
    padding: 16px;
    background: #fff3f3;
    border: 1px solid #f0cccc;
    border-radius: 6px;
    color: #e63946;
    font-size: 14px;
  }

  .abhinaya-track-order__error svg {
    flex: 0 0 auto;
  }

  /* ── Results ── */
  .abhinaya-track-order__result-header {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    gap: 12px;
    margin-bottom: 32px;
    padding-bottom: 24px;
    border-bottom: 1px solid #f0f0f0;
  }

  .abhinaya-track-order__result-meta {
    flex: 1;
  }

  .abhinaya-track-order__result-title {
    font-size: 18px;
    font-weight: 700;
    color: #1a1a2e;
    margin: 0 0 4px;
  }

  .abhinaya-track-order__result-info {
    font-size: 13px;
    color: #666;
    margin: 0;
  }

  .abhinaya-track-order__reset-btn {
    padding: 8px 12px;
    background: transparent;
    color: #1a1a2e;
    border: 1px solid #ddd;
    border-radius: 4px;
    font-size: 13px;
    font-weight: 600;
    cursor: pointer;
    transition: background 0.15s ease;
    font-family: inherit;
  }

  .abhinaya-track-order__reset-btn:active {
    background: #f9f9f9;
  }

  /* ── Timeline ── */
  .abhinaya-track-order__timeline {
    margin-bottom: 32px;
  }

  .abhinaya-track-order__timeline-title {
    font-size: 14px;
    font-weight: 700;
    text-transform: uppercase;
    color: #999;
    margin: 0 0 20px;
  }

  .abhinaya-track-order__timeline-track {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 16px;
    position: relative;
  }

  .abhinaya-track-order__timeline-track::before {
    content: '';
    position: absolute;
    left: 12px;
    top: 24px;
    bottom: 0;
    width: 1px;
    background: linear-gradient(to bottom, #2d8f4e, #ddd);
  }

  .abhinaya-track-order__timeline-step {
    position: relative;
    padding-left: 40px;
    padding-bottom: 16px;
  }

  .abhinaya-track-order__timeline-dot {
    position: absolute;
    left: 0;
    top: 0;
    width: 24px;
    height: 24px;
    border-radius: 50%;
    background: #f0f0f0;
    border: 2px solid #ddd;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #999;
    font-size: 10px;
  }

  .abhinaya-track-order__timeline-step--completed .abhinaya-track-order__timeline-dot {
    background: #2d8f4e;
    border-color: #2d8f4e;
    color: #fff;
  }

  .abhinaya-track-order__timeline-label {
    display: block;
    font-size: 14px;
    font-weight: 600;
    color: #1a1a2e;
    margin-bottom: 4px;
  }

  .abhinaya-track-order__timeline-date {
    display: block;
    font-size: 12px;
    color: #999;
  }

  /* ── Section Titles ── */
  .abhinaya-track-order__section-title {
    font-size: 14px;
    font-weight: 700;
    color: #1a1a2e;
    margin: 0 0 16px;
    text-transform: uppercase;
    letter-spacing: 0.5px;
  }

  /* ── Courier Details ── */
  .abhinaya-track-order__courier {
    background: #f9f9f9;
    border-radius: 8px;
    padding: 16px;
    margin-bottom: 24px;
  }

  .abhinaya-track-order__courier-info {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .abhinaya-track-order__courier-row {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 14px;
  }

  .abhinaya-track-order__courier-label {
    color: #666;
  }

  .abhinaya-track-order__courier-value {
    font-weight: 600;
    color: #1a1a2e;
  }

  .abhinaya-track-order__courier-link {
    display: flex;
    align-items: center;
    gap: 8px;
    padding: 12px;
    background: #1a1a2e;
    color: #fff;
    text-decoration: none;
    border-radius: 6px;
    font-size: 13px;
    font-weight: 600;
    margin-top: 4px;
    transition: opacity 0.15s ease;
  }

  .abhinaya-track-order__courier-link:active {
    opacity: 0.85;
  }

  /* ── Delivery Estimate ── */
  .abhinaya-track-order__delivery-estimate {
    margin-bottom: 24px;
  }

  .abhinaya-track-order__estimate-card {
    background: #f0f7f4;
    border-left: 4px solid #2d8f4e;
    border-radius: 6px;
    padding: 16px;
    text-align: center;
  }

  .abhinaya-track-order__estimate-date {
    display: block;
    font-size: 18px;
    font-weight: 700;
    color: #2d8f4e;
    margin-bottom: 4px;
  }

  .abhinaya-track-order__estimate-info {
    font-size: 12px;
    color: #666;
    margin: 0;
  }

  /* ── Items List ── */
  .abhinaya-track-order__items {
    margin-bottom: 32px;
  }

  .abhinaya-track-order__items-list {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .abhinaya-track-order__item {
    display: flex;
    gap: 12px;
    padding: 12px;
    background: #f9f9f9;
    border-radius: 6px;
  }

  .abhinaya-track-order__item-image {
    flex: 0 0 auto;
    width: 60px;
    height: 80px;
    border-radius: 4px;
    overflow: hidden;
    background: #fff;
  }

  .abhinaya-track-order__item-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .abhinaya-track-order__item-info {
    flex: 1;
  }

  .abhinaya-track-order__item-title {
    font-size: 13px;
    font-weight: 600;
    color: #1a1a2e;
    margin: 0 0 4px;
  }

  .abhinaya-track-order__item-variant {
    font-size: 12px;
    color: #666;
    margin: 0;
  }

  /* ── Support ── */
  .abhinaya-track-order__support {
    background: #f9f9f9;
    border-radius: 8px;
    padding: 20px 16px;
    text-align: center;
  }

  .abhinaya-track-order__support-buttons {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .abhinaya-track-order__support-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 14px 16px;
    border-radius: 6px;
    text-decoration: none;
    font-size: 14px;
    font-weight: 600;
    min-height: 48px;
    transition: opacity 0.15s ease;
  }

  .abhinaya-track-order__support-btn--whatsapp {
    background: #25d366;
    color: #fff;
  }

  .abhinaya-track-order__support-btn--whatsapp:active {
    opacity: 0.85;
  }

  .abhinaya-track-order__support-btn--email {
    background: #1a1a2e;
    color: #fff;
  }

  .abhinaya-track-order__support-btn--email:active {
    opacity: 0.85;
  }

  /* ── Desktop ── */
  @media (min-width: 750px) {
    .abhinaya-track-order {
      max-width: 600px;
      margin: 0 auto;
      padding: 40px 24px 80px;
    }

    .abhinaya-track-order__timeline-track {
      grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
      gap: 12px;
    }

    .abhinaya-track-order__timeline-track::before {
      left: 12px;
      width: calc(100% - 24px);
      top: 24px;
      bottom: auto;
      height: 1px;
    }

    .abhinaya-track-order__timeline-step {
      padding-left: 0;
      padding-top: 40px;
      text-align: center;
    }

    .abhinaya-track-order__timeline-dot {
      position: relative;
      left: auto;
      top: auto;
      margin: 0 auto 16px;
    }

    .abhinaya-track-order__support-buttons {
      flex-direction: row;
      gap: 12px;
    }

    .abhinaya-track-order__support-btn {
      flex: 1;
    }
  }

  @media (prefers-reduced-motion: reduce) {
    .abhinaya-track-order__search-btn,
    .abhinaya-track-order__courier-link,
    .abhinaya-track-order__support-btn {
      transition: none;
    }
  }
</style>

<script>
  (function() {
    'use strict';

    var form = document.getElementById('abhinayaTrackForm');
    var orderIdInput = document.getElementById('abhinayaOrderId');
    var phoneInput = document.getElementById('abhinayaPhone');
    var error = document.getElementById('abhinayaTrackError');
    var errorText = document.getElementById('abhinayaTrackErrorText');
    var formSection = document.querySelector('.abhinaya-track-order__form-section');
    var results = document.getElementById('abhinayaTrackResults');
    var resetBtn = document.getElementById('abhinayaTrackReset');

    function showError(msg) {
      if (errorText) errorText.textContent = msg;
      if (error) error.hidden = false;
    }

    function hideError() {
      if (error) error.hidden = true;
    }

    function displayResults(order) {
      if (document.getElementById('abhinayaResultOrderId')) {
        document.getElementById('abhinayaResultOrderId').textContent = '#' + order.id;
      }
      if (document.getElementById('abhinayaResultStatus')) {
        document.getElementById('abhinayaResultStatus').textContent = order.status;
      }
      if (document.getElementById('abhinayaEstimatedDate')) {
        document.getElementById('abhinayaEstimatedDate').textContent = order.estimatedDelivery;
      }
      if (document.getElementById('abhinayaCourierName')) {
        document.getElementById('abhinayaCourierName').textContent = order.courier;
      }
      if (document.getElementById('abhinayaCourierTrackingId')) {
        document.getElementById('abhinayaCourierTrackingId').textContent = order.trackingId;
      }

      // Update timeline
      var steps = order.currentStep;
      for (var i = 1; i <= 5; i++) {
        var step = document.getElementById('abhinayaStep' + i);
        if (step) {
          if (i <= steps) {
            step.classList.add('abhinaya-track-order__timeline-step--completed');
          } else {
            step.classList.remove('abhinaya-track-order__timeline-step--completed');
          }
        }
      }

      // Show courier details if shipped
      if (steps >= 3) {
        var courierDetails = document.getElementById('abhinayaCourierDetails');
        if (courierDetails) courierDetails.hidden = false;
      }

      // Populate items
      var itemsList = document.getElementById('abhinayaResultItems');
      if (itemsList) {
        itemsList.innerHTML = '';
        order.items.forEach(function(item) {
          var itemEl = document.createElement('div');
          itemEl.className = 'abhinaya-track-order__item';
          itemEl.innerHTML = '<div class="abhinaya-track-order__item-image"><img src="' + item.image + '" alt="" loading="lazy"></div>' +
            '<div class="abhinaya-track-order__item-info">' +
            '<h4 class="abhinaya-track-order__item-title">' + item.title + '</h4>' +
            '<p class="abhinaya-track-order__item-variant">' + item.variant + '</p>' +
            '</div>';
          itemsList.appendChild(itemEl);
        });
      }

      if (formSection) formSection.hidden = true;
      if (results) results.hidden = false;
    }

    if (form) {
      form.addEventListener('submit', function(e) {
        e.preventDefault();
        hideError();

        var orderId = orderIdInput ? orderIdInput.value.replace(/[^0-9]/g, '') : '';
        var phone = phoneInput ? phoneInput.value.replace(/\D/g, '') : '';

        if (!orderId) {
          showError('Please enter an order ID');
          return;
        }
        if (phone.length !== 10) {
          showError('Please enter a valid 10-digit phone number');
          return;
        }

        // Mock API call - in production, fetch real order data
        setTimeout(function() {
          displayResults({
            id: orderId,
            status: 'In transit to your location',
            currentStep: 3,
            courier: 'Delhivery',
            trackingId: 'DL' + orderId,
            estimatedDelivery: 'Jan 15 - Jan 17, 2025',
            items: [
              {
                title: 'Cotton Saree',
                variant: 'Maroon with gold border',
                image: 'https://via.placeholder.com/60x80?text=Saree'
              }
            ]
          });
        }, 300);
      });
    }

    if (resetBtn) {
      resetBtn.addEventListener('click', function() {
        if (orderIdInput) orderIdInput.value = '';
        if (phoneInput) phoneInput.value = '';
        hideError();
        if (formSection) formSection.hidden = false;
        if (results) results.hidden = true;
        if (orderIdInput) orderIdInput.focus();
      });
    }
  })();
</script>

