{%- comment -%}
  Abhinaya Track Order Section - Standalone page for customers to track their orders
  Features:
  - Order lookup form (Order ID + Phone)
  - Order status timeline with real-time tracking
  - Estimated delivery date
  - Courier details and tracking link
  - Help and support CTAs
{%- endcomment -%}

<div class="abhinaya-track-order">
  <!-- Header -->
  <header class="abhinaya-track-order__header">
    <h1 class="abhinaya-track-order__title">Track Your Order</h1>
    <p class="abhinaya-track-order__subtitle">Enter your order details to see the latest status</p>
  </header>

  <!-- Lookup Form -->
  <section class="abhinaya-track-order__form-section">
    <form class="abhinaya-track-order__form" id="abhinayaTrackForm">
      <div class="abhinaya-track-order__form-group">
        <label for="abhinayaOrderId" class="abhinaya-track-order__label">Order ID</label>
        <input type="text" id="abhinayaOrderId" class="abhinaya-track-order__input" placeholder="e.g. #12345" required>
        <span class="abhinaya-track-order__input-hint">Found in your confirmation email</span>
      </div>

      <div class="abhinaya-track-order__form-group">
        <label for="abhinayaPhone" class="abhinaya-track-order__label">Phone Number</label>
        <div class="abhinaya-track-order__phone-wrapper">
          <span class="abhinaya-track-order__country-code">+91</span>
          <input type="tel" id="abhinayaPhone" class="abhinaya-track-order__input" placeholder="10-digit mobile number" maxlength="10" inputmode="numeric" pattern="[0-9]*" required>
        </div>
      </div>

      <button type="submit" class="abhinaya-track-order__search-btn">Track Order</button>
    </form>

    <!-- Error State -->
    <div class="abhinaya-track-order__error" id="abhinayaTrackError" hidden>
      <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
        <circle cx="12" cy="12" r="10"/>
        <line x1="12" y1="8" x2="12" y2="12"/>
        <line x1="12" y1="16" x2="12.01" y2="16"/>
      </svg>
      <span id="abhinayaTrackErrorText">Order not found. Please check your details.</span>
    </div>
  </section>

  <!-- Results Section (shown after successful lookup) -->
  <section class="abhinaya-track-order__results" id="abhinayaTrackResults" hidden>
    <!-- Order Header -->
    <div class="abhinaya-track-order__result-header">
      <div class="abhinaya-track-order__result-meta">
        <h2 class="abhinaya-track-order__result-title" id="abhinayaResultOrderId"></h2>
        <p class="abhinaya-track-order__result-info" id="abhinayaResultStatus"></p>
      </div>
      <button class="abhinaya-track-order__reset-btn" id="abhinayaTrackReset">← Back</button>
    </div>

    <!-- Delivery Timeline -->
    <div class="abhinaya-track-order__timeline">
      <h3 class="abhinaya-track-order__timeline-title">Shipment Status</h3>
      <div class="abhinaya-track-order__timeline-track">
        <!-- Step 1: Order Confirmed -->
        <div class="abhinaya-track-order__timeline-step abhinaya-track-order__timeline-step--completed" id="abhinayaStep1">
          <div class="abhinaya-track-order__timeline-dot">
            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <polyline points="20 6 9 17 4 12"/>
            </svg>
          </div>
          <span class="abhinaya-track-order__timeline-label">Order Confirmed</span>
          <span class="abhinaya-track-order__timeline-date" id="abhinayaStep1Date">Today</span>
        </div>

        <!-- Step 2: Packed -->
        <div class="abhinaya-track-order__timeline-step" id="abhinayaStep2">
          <div class="abhinaya-track-order__timeline-dot">
            <span></span>
          </div>
          <span class="abhinaya-track-order__timeline-label">Packed</span>
          <span class="abhinaya-track-order__timeline-date" id="abhinayaStep2Date">Soon</span>
        </div>

        <!-- Step 3: Shipped -->
        <div class="abhinaya-track-order__timeline-step" id="abhinayaStep3">
          <div class="abhinaya-track-order__timeline-dot">
            <span></span>
          </div>
          <span class="abhinaya-track-order__timeline-label">Shipped</span>
          <span class="abhinaya-track-order__timeline-date" id="abhinayaStep3Date">Pending</span>
        </div>

        <!-- Step 4: In Transit -->
        <div class="abhinaya-track-order__timeline-step" id="abhinayaStep4">
          <div class="abhinaya-track-order__timeline-dot">
            <span></span>
          </div>
          <span class="abhinaya-track-order__timeline-label">In Transit</span>
          <span class="abhinaya-track-order__timeline-date" id="abhinayaStep4Date">Pending</span>
        </div>

        <!-- Step 5: Delivered -->
        <div class="abhinaya-track-order__timeline-step" id="abhinayaStep5">
          <div class="abhinaya-track-order__timeline-dot">
            <span></span>
          </div>
          <span class="abhinaya-track-order__timeline-label">Delivered</span>
          <span class="abhinaya-track-order__timeline-date" id="abhinayaStep5Date">Pending</span>
        </div>
      </div>
    </div>

    <!-- Courier Details -->
    <div class="abhinaya-track-order__courier" id="abhinayaCourierDetails" hidden>
      <h3 class="abhinaya-track-order__section-title">Courier Details</h3>
      <div class="abhinaya-track-order__courier-info">
        <div class="abhinaya-track-order__courier-row">
          <span class="abhinaya-track-order__courier-label">Courier</span>
          <span class="abhinaya-track-order__courier-value" id="abhinayaCourierName">Delhivery</span>
        </div>
        <div class="abhinaya-track-order__courier-row">
          <span class="abhinaya-track-order__courier-label">Tracking Number</span>
          <span class="abhinaya-track-order__courier-value" id="abhinayaCourierTrackingId">DL12345678</span>
        </div>
        <a href="#" class="abhinaya-track-order__courier-link" id="abhinayaCourierLink" target="_blank">
          Track on Courier Website
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6"/>
            <polyline points="15 3 21 3 21 9"/>
            <line x1="10" y1="14" x2="21" y2="3"/>
          </svg>
        </a>
      </div>
    </div>

    <!-- Estimated Delivery -->
    <div class="abhinaya-track-order__delivery-estimate">
      <h3 class="abhinaya-track-order__section-title">Estimated Delivery</h3>
      <div class="abhinaya-track-order__estimate-card">
        <span class="abhinaya-track-order__estimate-date" id="abhinayaEstimatedDate">Jan 15 - Jan 17</span>
        <p class="abhinaya-track-order__estimate-info">Delivery to your address</p>
      </div>
    </div>

    <!-- Order Items -->
    <div class="abhinaya-track-order__items">
      <h3 class="abhinaya-track-order__section-title">Order Items</h3>
      <div class="abhinaya-track-order__items-list" id="abhinayaResultItems"></div>
    </div>

    <!-- Support CTA -->
    <div class="abhinaya-track-order__support">
      <h3 class="abhinaya-track-order__section-title">Need Help?</h3>
      <div class="abhinaya-track-order__support-buttons">
        <a href="https://wa.me/911234567890" class="abhinaya-track-order__support-btn abhinaya-track-order__support-btn--whatsapp" target="_blank">
          <svg width="18" height="18" viewBox="0 0 24 24" fill="currentColor">
            <path d="M17.472 14.382c-.297-.149-1.758-.867-2.03-.967-.273-.099-.471-.148-.67.15-.197.297-.767.966-.94 1.164-.173.199-.347.223-.644.075-.297-.15-1.255-.463-2.39-1.475-.883-.788-1.48-1.761-1.653-2.059-.173-.297-.018-.458.13-.606.134-.133.298-.347.446-.52.149-.174.198-.298.298-.497.099-.198.05-.371-.025-.52-.075-.149-.669-1.612-.916-2.207-.242-.579-.487-.5-.67-.51-.173-.008-.371-.01-.57-.01-.198 0-.52.074-.792.372-.272.297-1.04 1.016-1.04 2.479 0 1.462 1.065 2.875 1.213 3.074.149.198 2.096 3.2 5.076 4.487.709.306 1.262.489 1.694.625.712.227 1.36.195 1.871.118.571-.085 1.758-.719 2.006-1.413.248-.694.248-1.289.173-1.413-.074-.124-.272-.198-.57-.347m-5.421-7.403h-.004a9.87 9.87 0 00-9.746 9.798c0 2.734.732 5.41 2.124 7.738L.929 23.5l8.272-2.737c2.194 1.14 4.656 1.742 7.15 1.742 9.563 0 17.35-7.787 17.35-17.349 0-4.63-1.773-8.981-4.979-12.263-3.206-3.283-7.534-5.084-12.102-5.084z"/>
          </svg>
          WhatsApp Support
        </a>
        <a href="/pages/contact" class="abhinaya-track-order__support-btn abhinaya-track-order__support-btn--email">
          <svg width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <rect x="2" y="4" width="20" height="16" rx="2"/>
            <path d="m22 7-8.97 5.7a1.94 1.94 0 0 1-2.06 0L2 7"/>
          </svg>
          Email Us
        </a>
      </div>
    </div>
  </section>
</div>

<style>
  .abhinaya-track-order {
    padding: 24px 16px 80px;
    max-width: 100%;
  }

  /* ── Header ── */
  .abhinaya-track-order__header {
    text-align: center;
    margin-bottom: 32px;
    padding: 24px 0;
    border-bottom: 1px solid #f0f0f0;
  }

  .abhinaya-track-order__title {
    font-size: 24px;
    font-weight: 700;
    color: #1a1a2e;
    margin: 0 0 8px;
  }

  .abhinaya-track-order__subtitle {
    font-size: 14px;
    color: #666;
    margin: 0;
  }

  /* ── Form ── */
  .abhinaya-track-order__form-section {
    margin-bottom: 40px;
    background: #f9f9f9;
    padding: 24px 16px;
    border-radius: 8px;
  }

  .abhinaya-track-order__form {
    display: flex;
    flex-direction: column;
    gap: 16px;
  }

  .abhinaya-track-order__form-group {
    display: flex;
    flex-direction: column;
    gap: 8px;
  }

  .abhinaya-track-order__label {
    font-size: 14px;
    font-weight: 600;
    color: #1a1a2e;
    display: block;
  }

  .abhinaya-track-order__input {
    padding: 12px 12px;
    border: 1px solid #ddd;
    border-radius: 6px;
    font-size: 16px;
    color: #1a1a2e;
    outline: none;
    transition: border-color 0.15s ease;
    font-family: inherit;
  }

  .abhinaya-track-order__input:focus {
    border-color: #1a1a2e;
    box-shadow: 0 0 0 3px rgba(26, 26, 46, 0.1);
  }

  .abhinaya-track-order__input.is-error {
    border-color: #e63946;
  }

  .abhinaya-track-order__input-hint {
    font-size: 12px;
    color: #999;
  }

  .abhinaya-track-order__phone-wrapper {
    display: flex;
    gap: 8px;
    align-items: stretch;
  }

  .abhinaya-track-order__country-code {
    display: flex;
    align-items: center;
    padding: 12px;
    background: #fff;
    border: 1px solid #ddd;
    border-radius: 6px;
    font-size: 16px;
    font-weight: 600;
    color: #1a1a2e;
    flex: 0 0 auto;
  }

  .abhinaya-track-order__phone-wrapper .abhinaya-track-order__input {
    flex: 1 1 auto;
  }

  .abhinaya-track-order__search-btn {
    padding: 14px 16px;
    background: #1a1a2e;
    color: #fff;
    border: none;
    border-radius: 6px;
    font-size: 14px;
    font-weight: 600;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    min-height: 48px;
